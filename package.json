{
  "name": "hubot-ollama",
  "description": "Hubot script for integrating with Ollama local LLMs",
  "version": "1.6.0",
  "author": "Stephen Yeargin <stephen@yearg.in>",
  "homepage": "https://github.com/stephenyeargin/hubot-ollama",
  "license": "MIT",
  "keywords": [
    "hubot",
    "hubot-scripts",
    "ollama",
    "llm",
    "ai"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/stephenyeargin/hubot-ollama.git"
  },
  "bugs": {
    "url": "https://github.com/stephenyeargin/hubot-ollama/issues"
  },
  "peerDependencies": {
    "hubot": ">=3 || 0.0.0-development"
  },
  "files": [
    "src/**/*.js"
  ],
  "devDependencies": {
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.4.0",
    "hubot": "^4",
    "hubot-test-helper": "^1.9.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "nock": "^14.0.10"
  },
  "main": "index.js",
  "scripts": {
    "test": "script/test",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/helpers/",
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "clover"
    ],
    "coverageDirectory": "./test-results",
    "collectCoverage": true
  },
  "volta": {
    "node": "24.10.0"
  },
  "dependencies": {
    "ascii-table": "^0.0.9",
    "marked": "^15.0.6",
    "ollama": "^0.6.3"
  }
